<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="550.0" prefWidth="800.0" styleClass="pane"
            xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.example.javachartfx.BarChartController">
    <children>
        <BarChart fx:id="barChart" barGap="5.0" layoutX="50.0" layoutY="46.0" prefHeight="420.0" prefWidth="700.0"
                  titleSide="BOTTOM" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"
                  AnchorPane.topAnchor="50.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" minorTickCount="10" side="LEFT" tickUnit="10.0"/>
            </yAxis>
        </BarChart>
        <Button fx:id="tableViewBtn" layoutX="650.0" layoutY="494.0" mnemonicParsing="false"
                onAction="#switchToTableView" prefHeight="29.0" prefWidth="100.0" text="View Table"
                AnchorPane.rightAnchor="50.0"/>
    </children>
</AnchorPane>
